{
  "name": "nextjs-antd-breadcrumbs",
  "version": "1.2.0",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "author": "luozhu",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngjuning/nextjs-antd-breadcrumbs.git"
  },
  "bugs": {
    "url": "https://github.com/youngjuning/nextjs-antd-breadcrumbs/issues"
  },
  "homepage": "https://github.com/youngjuning/nextjs-antd-breadcrumbs#readme",
  "scripts": {
    "dev": "next dev",
    "build": "microbundle-crl --no-compress --css-modules null --format modern,cjs,umd --tsconfig tsconfig.build.json",
    "start": "next start",
    "lint": "next lint",
    "commit": "git cz",
    "prepublishOnly": "yarn build",
    "release-it": "standard-version"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "gitHooks": {
    "commit-msg": "commitlint -e -V"
  },
  "dependencies": {
    "antd": "4.17.3",
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@types/node": "16.11.13",
    "@types/react": "17.0.37",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cz-customizable": "^6.3.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "microbundle-crl": "^0.13.11",
    "standard-version": "^9.3.2",
    "typescript": "4.5.4",
    "yorkie": "^2.0.0"
  }
}
